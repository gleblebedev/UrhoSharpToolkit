<Application x:Class="UrhoSharp.Editor.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:UrhoSharp.Editor"
             Startup="App_OnStartup"
             Exit="App_OnExit">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/View/Styles/Dark.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/View/Styles/WinStyle/WinStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>